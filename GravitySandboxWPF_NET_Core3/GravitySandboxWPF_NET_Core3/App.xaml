<Application x:Class="GravitySandboxWPF_NET_Core3.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:GravitySandboxWPF_NET_Core3"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>

            <SolidColorBrush x:Key="bodyColorDirt" Color="#FFB2A87B" />
            <SolidColorBrush x:Key="monitoredBodyColor" Color="White" />

            <SolidColorBrush x:Key="bodyColor1" Color="#FFFFA0C7" />
            <SolidColorBrush x:Key="bodyColor2" Color="#FFA0FFA4" />
            <SolidColorBrush x:Key="bodyColor3" Color="#FFA0EEFF" />
            <SolidColorBrush x:Key="bodyColor4" Color="#FFE1A0FF" />
            <SolidColorBrush x:Key="bodyColor5" Color="#FFF6FFA0" />
            <SolidColorBrush x:Key="bodyColor6" Color="#FFFFD8A0" />
            <SolidColorBrush x:Key="bodyColor7" Color="#FFA0A4FF" />
            <SolidColorBrush x:Key="bodyColor8" Color="#FFFFA0A0" />
            <SolidColorBrush x:Key="bodyColor9" Color="#FF646464" />
            <SolidColorBrush x:Key="bodyColor10" Color="#FFA0A0A0" />
            <SolidColorBrush x:Key="bodyColor11" Color="#FFD8D8D8" />

        </ResourceDictionary>

    </Application.Resources>
</Application>
